{
  "name": "webpack-tailwind-starter",
  "version": "0.0.1",
  "description": "A starting point for testing out TailwindCSS",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "NODE_ENV='dev' env UV_THREADPOOL_SIZE=100 webpack --display-error-details -d --watch  --config webpack/webpack.dev.js",
    "serve": "webpack-dev-server --config webpack/webpack.dev.js --mode=production",
    "prod": "NODE_ENV='prod' env UV_THREADPOOL_SIZE=100 webpack -p --config webpack/webpack.prod.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/welance/webpack-tailwind-starter.git"
  },
  "keywords": [
    "webpack",
    "tailwind",
    "CSS",
    "postCSS"
  ],
  "author": "Enrico Icardi <enrico@welance.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/welance/webpack-tailwind-starter/issues"
  },
  "homepage": "https://github.com/welance/webpack-tailwind-starter#readme",
  "devDependencies": {
    "autoprefixer": "^8.2.0",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "cssnano": "^3.10.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-plugin": "^3.1.0",
    "mini-css-extract-plugin": "^0.4.0",
    "path": "^0.12.7",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.3",
    "style-loader": "^0.20.3",
    "tailwindcss": "^0.5.2",
    "webpack": "^4.4.1",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {}
}
